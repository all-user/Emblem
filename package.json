{
  "name": "@all-user/ok-blocks",
  "version": "0.0.7",
  "description": "Generate blocks consists of 3x3 elements.",
  "main": "index.js",
  "scripts": {
    "help": "npm-run-help",
    "test": "testem",
    "lint:src": "eslint src/js/**/*.js",
    "lint:test": "eslint test/**/*.test.js",
    "lint": "npm-run-all -p lint:*",
    "build:js": "babel src/js -x .js -d lib",
    "watch:js": "babel -w src/js -x .js -d lib",
    "release:js": "browserify -t babelify -r ./src/js/index.js:@all-user/ok-blocks | uglifyjs > ./dist/bundle.js",
    "release:clean": "rimraf ./dist",
    "release:mkdir": "npm run release:clean && mkdirp ./dist",
    "release": "npm-run-all -s release:mkdir -p release:js",
    "build": "npm run build:js",
    "watch": "npm run watch:js",
    "start": "npm run watch"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "@all-user/ok-patterns-lines": "0.0.3",
    "@all-user/ok-patterns-olympic2020": "0.0.3",
    "append-css": "^0.1.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-espower": "^2.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cpx": "^1.3.1",
    "eslint": "^2.7.0",
    "exorcist": "^0.4.0",
    "mkdirp": "*",
    "mocha": "^2.4.5",
    "nib": "^1.1.0",
    "npm-run-all": "^1.7.0",
    "npm-run-help": "^1.1.0",
    "power-assert": "^1.3.1",
    "rimraf": "^2.5.2",
    "stylus": "^0.54.2",
    "testem": "*",
    "uglify-js": "^2.6.2",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "xtend": "^4.0.1"
  }
}
