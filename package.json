{
  "name": "@all-user/ok-blocks",
  "version": "0.0.1",
  "description": "Generate blocks consists of 3x3 elements.",
  "main": "index.js",
  "scripts": {
    "test": "testem",
    "lint:src": "eslint src/js/**/*.js",
    "lint:test": "eslint test/**/*.test.js",
    "lint": "npm-run-all -p lint:*",
    "build:js": "browserify -t babelify ./src/js/index.js -d | exorcist ./src/bundle.js.map > ./src/bundle.js",
    "watch:js": "watchify -v -t babelify ./src/js/index.js -o \"exorcist ./src/bundle.js.map > ./src/bundle.js\" -d",
    "release:js": "browserify -t babelify ./src/js/index.js | uglifyjs > ./dist/bundle.js",
    "release:es5": "babel src/js -x .js -d lib",
    "release:clean": "rimraf ./dist",
    "release:mkdir": "npm run release:clean && mkdirp ./dist",
    "release": "npm-run-all -s release:mkdir -p release:js release:es5",
    "watch": "npm run watch:js",
    "start": "npm run watch"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "append-css": "^0.1.6",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.6",
    "babel-plugin-espower": "^2.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.20",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "cpx": "^1.2.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.2",
    "exorcist": "^0.4.0",
    "mkdirp": "*",
    "mocha": "^2.2.5",
    "nib": "^1.1.0",
    "npm-run-all": "^1.2.12",
    "power-assert": "^1.2.0",
    "rimraf": "^2.4.3",
    "stylus": "^0.52.0",
    "testem": "*",
    "uglify-js": "^2.4.24",
    "watch": "^0.16.0",
    "watchify": "^3.3.1"
  },
  "dependencies": {
    "xtend": "^4.0.0",
    "@all-user/ok-patterns-lines": "file:../ok-patterns-lines",
    "@all-user/ok-patterns-olympic2020": "file:../ok-patterns-olympic2020"
  }
}
