{
  "name": "@all-user/ok-blocks",
  "version": "0.0.7",
  "description": "Generate blocks consists of 3x3 elements.",
  "main": "lib/js/index.js",
  "scripts": {
    "help": "npm-run-help",
    "test": "testem",
    "install": "run-s install:*",
    "install:types": "if [ -L node_modules/@all-user/ok-blocks.types ]; then :; else npm run setup:types; fi",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "setup": "run-s setup:*",
    "setup:types": "rm -rf node_modules/@all-user/ok-blocks.types && mkdirp node_modules/@all-user && ln -fs `pwd`/packages/@all-user/ok-blocks.types node_modules/@all-user/ok-blocks.types",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/js/**/*.js",
    "lint:test": "eslint test/**/*.test.js",
    "build:js": "babel src/js -x .js -d lib/js",
    "watch:js": "babel -w src/js -x .js -d lib/js",
    "release:js": "browserify -t babelify -r ./src/js/index.js:@all-user/ok-blocks | uglifyjs > ./dist/bundle.js",
    "release:clean": "rimraf ./dist && mkdirp ./dist",
    "release": "run-s release:clean release:js",
    "build": "run-s build:*",
    "watch": "run-p watch:*",
    "bumpup": "bin/bumpup @all-user/ok-blocks.types",
    "start": "npm run watch"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "@all-user/ok-patterns-lines": "0.0.3",
    "@all-user/ok-patterns-olympic2020": "0.0.3",
    "append-css": "^0.1.6",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "cpx": "^1.3.1",
    "eslint": "^3.15.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "mkdirp": "*",
    "mocha": "^2.4.5",
    "nib": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "npm-run-help": "^1.1.0",
    "power-assert": "^1.4.2",
    "rimraf": "^2.5.2",
    "stylus": "^0.54.2",
    "testem": "*",
    "uglify-js": "^2.6.2",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "xtend": "^4.0.1"
  }
}
